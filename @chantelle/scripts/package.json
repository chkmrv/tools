    "@chantelle/util": "^0.0.1-5",
    "babel-core": "^6.26.0",
    "dotenv": "^4.0.0",
    "fs-extra": "^4.0.2",
    "glob": "^7.1.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "react-dev-utils": "^4.2.1",
    "shelljs": "^0.7.8",
    "webpack-dev-server": "^2.9.4"
  },
  "devDependencies": {
    "@chantelle/eslint-config-chantelle": "^0.0.1-5",
    "@chantelle/eslint-plugin-chantelle": "^0.0.1-5",
